label class="Block"><response_label ident="QUES_1458548_1494413_A5698661"><flow_mat><material><mattext texttype="text/html">&lt;p&gt;The purpose of the complementary filter is to low-pass filter noise from a single measurement.&lt;/p&gt;</mattext></material></flow_mat></response_label></flow_label><flow_label class="Block"><response_label ident="QUES_1458548_1494413_A5698662"><flow_mat><material><mattext texttype="text/html">&lt;p&gt;The purpose of the complementary filter is to high-pass filter noise from a single measurement.&lt;/p&gt;</mattext></material></flow_mat></response_label></flow_label><flow_label class="Block"><response_label ident="QUES_1458548_1494413_A5698663"><flow_mat><material><mattext texttype="text/html">&lt;p&gt;The purpose of the complementary filter is to band-pass filter a noisy signal.&lt;/p&gt;</mattext></material></flow_mat></response_label></flow_label></render_choice></response_lid></flow></presentation><resprocessing><respcondition title="Response Condition 1"><conditionvar><varequal respident="QUES_1458548_1494413_LID">QUES_1458548_1494413_A5698660</varequal></conditionvar><setvar action="Set">100.000000000</setvar><displayfeedback feedbacktype="Response" linkrefid="QUES_1458548_1494413_IF5698660" /></respcondition><respcondition title="Response Condition 2"><conditionvar><varequal respident="QUES_1458548_1494413_LID">QUES_1458548_1494413_A5698661</varequal></conditionvar><setvar action="Set">0.000000000</setvar><displayfeedback feedbacktype="Response" linkrefid="QUES_1458548_1494413_IF5698661" /></respcondition><respcondition title="Response Condition 3"><conditionvar><varequal respident="QUES_1458548_1494413_LID">QUES_1458548_1494413_A5698662</varequal></conditionvar><setvar action="Set">0.000000000</setvar><displayfeedback feedbacktype="Response" linkrefid="QUES_1458548_1494413_IF5698662" /></respcondition><respcondition title="Response Condition 4"><conditionvar><varequal respident="QUES_1458548_1494413_LID">QUES_1458548_1494413_A